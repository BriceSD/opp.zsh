#!zsh

declare F="${1}"
declare -a T
: ${(A)T::=${${${(ps:\n#:)${${"$(<$F)"}%# Code*}#\#}}}}
: ${(A)T::=${T# }}
: ${(A)T::=${T//</&lt;}}
: ${(A)T::=${T//>/&gt;}}
: ${(A)T::=${T//8&lt;/>8}}
: ${(A)T::=${T//&gt;8/8<}}

cat <<EOT
<img src="http://github.com/hchbaw/opp.zsh/raw/readme/ciw.png" />

<pre>
$(print -l $T)
</pre>
EOT
